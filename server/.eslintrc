{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "globals": {
    "$": true,
    "require": true,
    "process": true
  },
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "@typescript-eslint/no-var-requires": 0
}
}
